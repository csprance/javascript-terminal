{
  "name": "async-javascript-terminal",
  "version": "2.0.0",
  "description": "Emulate a terminal environment in JavaScript",
  "main": "lib/terminal.js",
  "unpkg": "lib/terminal.min.js",
  "scripts": {
    "build": "webpack --mode development && webpack --mode production",
    "test": "cross-env NODE_PATH=./src mocha --require babel-core/register --colors './test/**/*.spec.js' './demo-**/**/*.spec.js'",
    "test:min": "yarn run test --reporter min",
    "test:coverage": "nyc yarn run test",
    "cli": "node demo-cli",
    "artifact:test-coverage": "nyc --reporter=html yarn run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/csprance/javascript-terminal.git"
  },
  "keywords": [
    "terminal",
    "emulation"
  ],
  "author": "Rohan Chandra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/csprance/javascript-terminal/issues"
  },
  "homepage": "https://github.com/csprance/javascript-terminal",
  "nyc": {
    "exclude": [
      "test",
      "lib"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-immutable": "^2.0.0-alpha.1",
    "chai-spies": "^1.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "get-options": "^1.1.1",
    "immutable": "^4.0.0-rc.9",
    "minimatch": "^3.0.4",
    "minimatch-capture": "^1.1.0"
  },
  "files": [
    "lib/terminal.js",
    "lib/terminal.min.js"
  ]
}
